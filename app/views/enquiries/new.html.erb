<h1>New Enquiry</h1>

<%= form_with model: @enquiry, local: true do |f| %>
<div>
  <%= f.label :name %> <%= f.text_field :name %>
</div>
<div>
  <%= f.label :email %> <%= f.email_field :email %>
</div>
<div>
  <%= f.label :phone %> <%= f.telephone_field :phone %>
</div>
<div>
    <%= f.label :message %> <%= f.text_area :message %>
</div>
<div>
  <%= f.submit %>
</div>
<% end %>
